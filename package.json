{
  "name": "clones-cli",
  "version": "0.2.0",
  "description": "A read-only Git repository manager for exploration and reference",
  "keywords": [
    "git",
    "clone",
    "repository",
    "manager",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lunelson/clones-cli.git"
  },
  "license": "MIT",
  "author": {
    "name": "Lu Nelson"
  },
  "type": "module",
  "bin": {
    "clones": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepack": "npm run build",
    "prepublishOnly": "npm test",
    "release": "np",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "cfonts": "^3.3.1",
    "citty": "^0.1.6",
    "fast-wrap-ansi": "^0.2.0",
    "is-unicode-supported": "^2.1.0",
    "picocolors": "^1.0.0",
    "simple-git": "^3.27.0",
    "sisteransi": "^1.0.5",
    "update-notifier": "^7.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "@types/update-notifier": "^6.0.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "np": "^10.2.0",
    "prettier": "^3.8.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.53.1",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
